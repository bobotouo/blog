":20,"value":732},"短身份证号码(数字、字母x结尾)：",{"type":14,"tag":37,"props":734,"children":736},{"className":735},[],[737],{"type":20,"value":738},"^([0-9]){7,18}(x|X)?$ 或 ^\\d{8,18}|[0-9x]{8,18}|[0-9X]{8,18}?$",{"type":14,"tag":230,"props":740,"children":741},{},[742,744],{"type":20,"value":743},"帐号是否合法(字母开头，允许5-16字节，允许字母数字下划线)：",{"type":14,"tag":37,"props":745,"children":747},{"className":746},[],[748],{"type":20,"value":749},"^[a-zA-Z][a-zA-Z0-9_]{4,15}$",{"type":14,"tag":230,"props":751,"children":752},{},[753,755],{"type":20,"value":754},"密码(以字母开头，长度在6~18之间，只能包含字母、数字和下划线)：",{"type":14,"tag":37,"props":756,"children":758},{"className":757},[],[759],{"type":20,"value":760},"^[a-zA-Z]\\w{5,17}$",{"type":14,"tag":230,"props":762,"children":763},{},[764,766],{"type":20,"value":765},"强密码(必须包含大小写字母和数字的组合，不能使用特殊字符，长度在8-10之间)：",{"type":14,"tag":37,"props":767,"children":769},{"className":768},[],[770],{"type":20,"value":771},"^(?=.*\\d)(?=.*[a-z])(?=.*[A-Z]).{8,10}$",{"type":14,"tag":230,"props":773,"children":774},{},[775,777],{"type":20,"value":776},"日期格式：",{"type":14,"tag":37,"props":778,"children":780},{"className":779},[],[781],{"type":20,"value":782},"^\\d{4}-\\d{1,2}-\\d{1,2}",{"type":14,"tag":230,"props":784,"children":785},{},[786,788],{"type":20,"value":787},"一年的12个月(01～09和1～12)：",{"type":14,"tag":37,"props":789,"children":791},{"className":790},[],[792],{"type":20,"value":793},"^(0?[1-9]|1[0-2])$",{"type":14,"tag":230,"props":795,"children":796},{},[797,799],{"type":20,"value":798},"一个月的31天(01～09和1～31)：",{"type":14,"tag":37,"props":800,"children":802},{"className":801},[],[803],{"type":20,"value":804},"^((0?[1-9])|((1|2)[0-9])|30|31)$",{"type":14,"tag":230,"props":806,"children":807},{},[808],{"type":20,"value":809},"钱的输入格式：",{"type":14,"tag":230,"props":811,"children":812},{},[813,815],{"type":20,"value":814},"1.有四种钱的表示形式我们可以接受:\"10000.00\" 和 \"10,000.00\", 和没有 \"分\" 的 \"10000\" 和 \"10,000\"：",{"type":14,"tag":37,"props":816,"children":818},{"className":817},[],[819],